{
  "name": "bookstock",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "npm run test:setup && TEST_DATABASE_URL=\"postgresql://test:test@localhost:5433/bookstock_test\" vitest run",
    "test:watch": "npm run test:setup && TEST_DATABASE_URL=\"postgresql://test:test@localhost:5433/bookstock_test\" vitest --watch",
    "test:coverage": "npm run test:setup && TEST_DATABASE_URL=\"postgresql://test:test@localhost:5433/bookstock_test\" vitest --coverage",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "tsx scripts/db-reset.ts dev",
    "db:reset:test": "tsx scripts/db-reset.ts test",
    "db:reset:prod": "tsx scripts/db-reset.ts prod",
    "db:rollback": "tsx scripts/db-rollback.ts",
    "db:performance": "tsx scripts/db-performance-test.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "docker:test": "docker-compose --profile test up -d postgres-test",
    "docker:test-down": "docker-compose --profile test down",
    "db:setup": "npm run docker:up && sleep 5 && npm run db:migrate && npm run db:seed",
    "test:setup": "npm run docker:test && sleep 5 && DATABASE_URL=\"postgresql://test:test@localhost:5433/bookstock_test\" npx prisma db push",
    "dev:full": "npm run db:setup && npm run dev"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.0.0",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "date-fns": "^4.1.0",
    "isbn3": "^1.2.14",
    "lucide-react": "^0.460.0",
    "next": "15.5.3",
    "papaparse": "^5.4.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-dropzone": "^14.2.9",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.7.9",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.5.3",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.49",
    "prisma": "^6.0.0",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
